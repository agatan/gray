def fib(n: Int) -> Int {
    if n == 0 || n == 1 {
        n
    } else {
        fib(n-1) + fib(n-2)
    }
}

def fib2(n: Int) -> Int {
    let a = ref 0
    let b = ref 1
    let i = ref 0
    while @i < n {
        let next = @a + @b
        a := @b
        b := next
        i := @i + 1
    }
    @b
}

def main() {
    println(fib(1))
    println(fib(3))
    println(fib(10))

    println(fib2(1))
    println(fib2(3))
    println(fib2(10))
}
