def fib(n: Int) -> Int {
    if n == 0 || n == 1 {
        n
    } else {
        fib(n-1) + fib(n-2)
    }
}

def fib2(n: Int) -> Int {
    let a: Ref<Int> = ref 0
    let b = ref 1
    let i = ref 0
    while @i < n {
        let next = @a + @b
        a := @b
        b := next
        i := @i + 1
    }
    @b
}

def main() {
    print_int(fib(1))
    print_int(fib(3))
    print_int(fib(10))

    print_int(fib2(1))
    print_int(fib2(3))
    print_int(fib2(10))
}
